<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wordfindertwo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:name="MyApp">
        <activity
            android:name="com.example.wordfindertwo.MainMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.wordfindertwo.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.example.wordfindertwo.Score"
            android:label="@string/title_activity_score" >
        </activity>
        <activity
            android:name="com.example.wordfindertwo.FriendsTable"
            android:label="@string/title_activity_friend" >
        </activity>
        <activity
            android:name="com.example.wordfindertwo.Game"
            android:label="@string/title_activity_game" >
        </activity>
        <activity
            android:name="com.example.wordfindertwo.Dictionary"
            android:label="@string/title_activity_dictionary" >
        </activity>
        <activity
            android:name="com.example.wordfindertwo.PauseScreen"
            android:label="@string/title_activity_pause_screen" >
        </activity>
        <activity
            android:name="com.example.wordfindertwo.AfterGame"
            android:label="@string/title_activity_after_game" >
        </activity>
        <activity
            android:name="com.example.wordfindertwo.Pause"
            android:label="@string/title_activity_pause" >
        </activity>
    </application>

</manifest>
